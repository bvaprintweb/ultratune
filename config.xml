<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.bvaprintweb.medicalcenter"
        versionCode="10" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>BVA Medical Center</name>
    <description>BVA Medical Center</description>
    <author href="http://bvaprintweb.com" email="jun@bvaprintweb.com">Juniawan</author>
	<preference name="fullscreen" value="true" />
	<preference name="stay-in-webview" value="true" />
	<preference name="exit-on-suspend" value="true" />
	<preference name="splash-screen-duration" value="5000" />
	<preference name="load-url-timeout" value="60000" />
	<preference name="webviewbounce" value="false" />
	<icon src="assets/images/ico57.png" />
	<icon src="assets/images/ico57.png" gap:platform="ios" width="57" height="57" />
	<icon src="assets/images/ico72.png" gap:platform="ios" width="72" height="72" />
	<icon src="assets/images/ico114.png" gap:platform="ios" width="114" height="114" />
	<gap:splash src="assets/images/startup.png" />
	<gap:splash src="assets/images/startup.png" gap:platform="ios" width="320" height="480" />
</widget>
